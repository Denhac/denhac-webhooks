{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "anlutro/l4-settings": "^1.3",
        "beyondcode/laravel-websockets": "^1.14",
        "doctrine/dbal": "^3.7",
        "guzzlehttp/guzzle": "^7.2",
        "hashids/hashids": "^4.1",
        "laravel/framework": "^10.43",
        "laravel/horizon": "^5.22",
        "laravel/passport": "^11.10",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/tinker": "^2.8",
        "php-http/curl-client": "^2.3",
        "predis/predis": "^1.1",
        "quickbooks/v3-php-sdk": "^6.1",
        "sentry/sentry-laravel": "^3.4",
        "slack-php/slack-block-kit": "^0.19.0",
        "spatie/laravel-data": "2.2.3",
        "spatie/laravel-event-sourcing": "^7.3",
        "spatie/laravel-failed-job-monitor": "^4.2",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-webhook-client": "^3.2",
        "spatie/ssl-certificate": "^1.22",
        "stripe/stripe-php": "^7.94",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "ylsideas/feature-flags": "^2.4"
    },
    "require-dev": {
        "deployer/deployer": "^7.3",
        "fakerphp/faker": "^1.13.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
