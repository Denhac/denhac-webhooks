{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "anlutro/l4-settings": "^1.0",
        "beyondcode/laravel-websockets": "^1.11",
        "doctrine/dbal": "^2.12",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "hashids/hashids": "^4.1",
        "laravel/framework": "^8.50.0",
        "laravel/horizon": "^5.7.0",
        "laravel/passport": "^10.1",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.6",
        "php-http/curl-client": "^1.4",
        "predis/predis": "^1.1",
        "quickbooks/v3-php-sdk": "^6.1",
        "sentry/sentry-laravel": "^2.4.1",
        "slack-php/slack-block-kit": "^0.19.0",
        "spatie/laravel-data": "2.2.3",
        "spatie/laravel-event-sourcing": "^4.8.0",
        "spatie/laravel-failed-job-monitor": "^3.4",
        "spatie/laravel-queueable-action": "^2.10.3",
        "spatie/laravel-webhook-client": "^2.7.5",
        "spatie/ssl-certificate": "^1.22",
        "stripe/stripe-php": "^7.94",
        "ylsideas/feature-flags": "^1.4.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5.14",
        "fakerphp/faker": "^1.13.0",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.3.0",
        "phpunit/phpunit": "^9.5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
